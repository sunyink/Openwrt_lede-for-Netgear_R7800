@@
Config:
	choice
		prompt "SSL Library"
		default LUA_ECO_WOLFSSL

		config LUA_ECO_OPENSSL
			bool "OpenSSL"

		config LUA_ECO_WOLFSSL
			bool "wolfSSL"

		config LUA_ECO_MBEDTLS
			bool "mbedTLS"
	endchoice
@@

